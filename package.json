{
	"name": "jsonator",
	"description": "A Node JS app for manipulating JSON string text streams",
	"contributors": [
		"Mac Angell <mac.ang311@gmail.com>"
	],
	"version": "0.1.1",
	"dependencies": {
		"opter": "0.x.x",
		"underscore": "1.x.x"
	},
	"devDependencies": {
		"mocha": "1.x.x",
		"jshint": "0.x.x",
		"travis-cov": "0.2.x",
		"blanket": "1.0.x"
	},
	"keywords": [
		"JSON",
		"edit"
	],
	"engines": {
		"node": ">=0.8.0"
	},
	"bin": "./bin/jsonator",
	"repository": {
		"type": "git",
		"url": "https://github.com/mac-/jsonator"
	},
	"scripts": {
		"test": "make test && make test-cov",
		"blanket": {
			"pattern": "//^((?!\/node_modules\/)(?!\/test\/).)*$/ig",
			"onlyCwd": true,
			"data-cover-flags": {
				"branchTracking": true
			}
		},
		"travis-cov": { "threshold": 95 }
	}
}